<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.octavianlab.custom.archetype</groupId>
  <artifactId>octavianlab-base-project</artifactId>
  <version>1.0.0</version>
  <packaging>maven-archetype</packaging>
  <name>${project.groupId}:${project.artifactId}</name>
  <description>Maven archetype which can be used to generate a multi module JavaEE 8 project in java 11</description>
  <url>https://github.com/danielecerulli/octavianlab-base-project-archetype</url>
  
  <licenses>
  <license>
    <name>MIT License</name>
    <url>http://www.opensource.org/licenses/mit-license.php</url>
  </license>
</licenses>
  
  <developers>
    <developer>
      <name>Daniele Cerulli</name>
      <email>d.cerulli@octavianlab.com</email>
      <organization>Octavian Lab</organization>
      <organizationUrl>https://www.octavianlab.com/it/</organizationUrl>
    </developer>
  </developers>
  
  <scm>
  <connection>scm:git:git://github.com/danielecerulli/octavianlab-base-project-archetype.git</connection>
  <developerConnection>scm:git:ssh://github.com:danielecerulli/octavianlab-base-project-archetype.git</developerConnection>
  <!-- <url>http://github.com/danielecerulli/octavianlab-base-project-archetype/tree/release/stage</url> -->
  <tag>HEAD</tag>
 </scm>
 
 <distributionManagement>
  <snapshotRepository>
   <id>octavianlab-base-project</id>
   <url>https://oss.sonatype.org/content/repositories/snapshots</url>
  </snapshotRepository>
  <repository>
   <id>octavianlab-base-project</id>
   <url>https://oss.sonatype.org/service/local/staging/deploy/maven2/</url>
  </repository>
 </distributionManagement>
  
  <properties>
    <maven.compiler.source>11</maven.compiler.source>
    <maven.compiler.target>11</maven.compiler.target>
    <archetype-packaging.version>3.0.1</archetype-packaging.version>
  </properties>
  
  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.archetype</groupId>
        <artifactId>archetype-packaging</artifactId>
        <version>${archetype-packaging.version}</version>
      </extension>
    </extensions>
  </build>
</project>